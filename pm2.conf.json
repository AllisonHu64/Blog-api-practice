{
    "apps": {
        "name": "blog-server",
        "script": "bin/www.js",
        "watch": true,
        "ignore_watch": [
            "node_modules",
            "log"
        ],
        "instances": 4,
        "error_file": "logs/error.log",
        "out_file": "logs/out.log",
        "log_date_format": "YYYY-MM-DD HH:mm:ss"
    }
}